{
  "name": "unplugin-vue-reactivity-function",
  "version": "1.1.9",
  "packageManager": "pnpm@10.8.0",
  "description": "Reactivity function.",
  "type": "module",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zhiyuanzmj/unplugin-vue-reactivity-function#readme",
  "bugs": {
    "url": "https://github.com/zhiyuanzmj/unplugin-vue-reactivity-function/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhiyuanzmj/unplugin-vue-reactivity-function.git"
  },
  "author": "zhiyuanzmj",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./volar": {
      "require": "./dist/volar.cjs",
      "import": "./dist/volar.js"
    },
    "./volar-browser": {
      "require": "./dist/volar-browser.cjs",
      "import": "./dist/volar-browser.js"
    },
    "./raw": {
      "require": "./dist/raw.cjs",
      "import": "./dist/raw.js"
    },
    "./rolldown": {
      "require": "./dist/rolldown.cjs",
      "import": "./dist/rolldown.js"
    },
    "./rspack": {
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.js"
    },
    "./nuxt": {
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./astro": {
      "require": "./dist/astro.cjs",
      "import": "./dist/astro.js"
    },
    "./eslint": {
      "require": "./dist/eslint.cjs",
      "import": "./dist/eslint.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "play": "pnpm run -C ./playground dev",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsmc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.1",
    "@nuxt/schema": "^3.17.1",
    "@typescript-eslint/scope-manager": "^8.31.1",
    "@vue-macros/common": "3.0.0-beta.10",
    "estree-walker": "2.0.2",
    "oxc-parser": "^0.67.0",
    "ts-macro": "^0.1.25",
    "unplugin": "^2.3.2"
  },
  "devDependencies": {
    "@babel/types": "^7.27.1",
    "@sxzz/eslint-config": "^6.2.0",
    "@sxzz/prettier-config": "^2.2.1",
    "@ts-macro/tsc": "^0.1.25",
    "@types/babel__traverse": "^7.20.7",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.3",
    "@typescript-eslint/types": "^8.31.1",
    "@typescript-eslint/utils": "^8.31.1",
    "@vue-macros/reactivity-transform": "3.0.0-beta.10",
    "@vue-macros/test-utils": "3.0.0-beta.10",
    "bumpp": "^10.1.0",
    "change-case": "^5.4.4",
    "eslint": "^9.26.0",
    "eslint-define-config": "^2.1.0",
    "fast-glob": "^3.3.3",
    "prettier": "^3.5.3",
    "tsdown": "0.11.0-beta.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vitest": "^3.1.2",
    "vue": "^3.5.13"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
